<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= image_tag("profile.png", :alt => "profile", height: 128, width: 128) %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>

<p> ALL REQUESTS</p>
<table border = "1px">
  <td>
  <% @requests.each do |x| %>
      <tr> <%= x.name.to_s + " " + x.created_at.to_s %> <td>
  <% end %>
    </td>
</table>

